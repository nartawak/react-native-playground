{
  "name": "react_native_playground",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "rn-cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "npm run rn-cli -- start",
    "ios": "npm run rn-cli -- run-ios",
    "android": "npm run rn-cli -- run-android",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage && codecov",
    "lint": "eslint ./src ./index.*.js --ext .js --fix"
  },
  "dependencies": {
    "react": "~15.4.0-rc.4",
    "react-native": "0.40.0"
  },
  "jest": {
    "preset": "jest-react-native",
    "setupFiles": [
      "<rootDir>/node_modules/jest-react-native/build/setup.js",
      "<rootDir>/config/jest/setup-files/index.js"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-jest": "17.0.2",
    "babel-preset-react-native": "1.9.0",
    "codecov": "^1.0.1",
    "eslint": "3.6.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-react-native": "^2.0.0",
    "jest": "17.0.3",
    "jest-react-native": "17.0.3",
    "react-test-renderer": "15.3.2"
  }
}
